  "private": true,
  "dependencies": {
    "@aptos-labs/wallet-adapter-core": "^2.2.0",
    "@aptos-labs/wallet-adapter-react": "^1.1.0",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chroma-js": "^2.4.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "aptos": "^1.10.0",
    "petra-plugin-wallet-adapter": "^0.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2",
    "typescript": "^4.9.5",
    "web-vitals": "^3.3.1"
  },
  "homepage": "./",
  "scripts": {
    "start": "vite --port 3000",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "fmt": "prettier --write 'src/**/*.(ts|tsx)'",
    "generate-move-types": "graphql-codegen --config ./src/generated/food/codegen.ts && pnpm fmt",
    "generate-apiv2-client": "graphql-codegen --config ./src/generated/apiv2/codegen.ts && pnpm fmt"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "no-throw-literal": "off",
      "no-multi-str": "off",
      "no-empty-pattern": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/import-types-preset": "^2.2.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@types/node": "^20.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "graphql": "^16.5.0",
    "graphql-request": "^5.1.0",
    "prettier": "^2.8.8",
    "vite": "^4.3.8"
  }
}
